#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/eeeer/OneDrive/Documents/Foundry_Projects/Full_Stack_Web3_Cyfrin/html_buy_coffee/node_modules/.pnpm/esbuild@0.25.12/node_modules/esbuild/bin/node_modules:/mnt/c/Users/eeeer/OneDrive/Documents/Foundry_Projects/Full_Stack_Web3_Cyfrin/html_buy_coffee/node_modules/.pnpm/esbuild@0.25.12/node_modules/esbuild/node_modules:/mnt/c/Users/eeeer/OneDrive/Documents/Foundry_Projects/Full_Stack_Web3_Cyfrin/html_buy_coffee/node_modules/.pnpm/esbuild@0.25.12/node_modules:/mnt/c/Users/eeeer/OneDrive/Documents/Foundry_Projects/Full_Stack_Web3_Cyfrin/html_buy_coffee/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/eeeer/OneDrive/Documents/Foundry_Projects/Full_Stack_Web3_Cyfrin/html_buy_coffee/node_modules/.pnpm/esbuild@0.25.12/node_modules/esbuild/bin/node_modules:/mnt/c/Users/eeeer/OneDrive/Documents/Foundry_Projects/Full_Stack_Web3_Cyfrin/html_buy_coffee/node_modules/.pnpm/esbuild@0.25.12/node_modules/esbuild/node_modules:/mnt/c/Users/eeeer/OneDrive/Documents/Foundry_Projects/Full_Stack_Web3_Cyfrin/html_buy_coffee/node_modules/.pnpm/esbuild@0.25.12/node_modules:/mnt/c/Users/eeeer/OneDrive/Documents/Foundry_Projects/Full_Stack_Web3_Cyfrin/html_buy_coffee/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../esbuild/bin/esbuild" "$@"
else
  exec node  "$basedir/../esbuild/bin/esbuild" "$@"
fi
